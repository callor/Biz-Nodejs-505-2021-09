extends layout

block content
	style.
		section.main {
			display:flex;
			flex-wrap:wrap;

			justify-content:center;
			align-items:center;
		}
		div.table {
			width:200px;
			height:100px;
			border:1px solid green;
			margin:10px;

			text-align:center;
			line-height:100px;
		}
	- for(let i = 0 ; i < 12 ; i++)
		div.table !{i + 1} 번 테이블
